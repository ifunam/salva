+  def token_string(n)
+    if n.to_i > 1
+      s = ""
+      char64 = (('a'..'z').collect + ('A'..'Z').collect + ('0'..'9').collect + ['.','/']).freeze
+      n.times { s << char64[(Time.now.tv_usec * rand) % 64] }
+      s
+    end
+  end
